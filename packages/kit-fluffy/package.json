{
  "name": "@qwik-ui/fluffy",
  "version": "0.0.0",
  "description": "Qwik UI components library with styles",
  "publishConfig": {
    "access": "public"
  },
  "main": "./index.qwik.cjs",
  "qwik": "./index.qwik.mjs",
  "types": "./qwik-ui/packages/kit-fluffy/src/index.d.ts",
  "module": "./index.qwik.mjs",
  "type": "module",
  "exports": {
    ".": {
      "import": "./index.qwik.mjs",
      "require": "./index.qwik.cjs"
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "private": false,
  "scripts": {
    "build": "qwik build",
    "build-storybook": "storybook build",
    "build.lib": "vite build --mode lib",
    "build.types": "tsc --emitDeclarationOnly",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "release": "np",
    "start": "vite --open --mode ssr",
    "storybook": "storybook dev -p 6006",
    "qwik": "qwik"
  },
  "dependencies": {
    "@qwik-ui/cva": "workspace:*"
  },
  "peerDependencies": {
    "@builder.io/qwik": ">1.1.0"
  },
  "devDependencies": {
    "@builder.io/vite-plugin-macro": "~0.0.7",
    "@storybook/addon-essentials": "7.0.23",
    "@storybook/addon-links": "7.0.23",
    "@storybook/blocks": "7.0.23",
    "@storybook/builder-vite": "7.0.23",
    "@storybook/html": "7.0.23",
    "@storybook/html-vite": "7.0.23",
    "storybook": "7.0.23",
    "storybook-framework-qwik": "0.2.2"
  }
}

{
  "name": "@qwik-ui/headless",
  "version": "0.4.0",
  "description": "Qwik UI headless components library",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qwikifiers/qwik-ui",
    "directory": "packages/kit-headless"
  },
  "main": "./index.qwik.cjs",
  "qwik": "./index.qwik.mjs",
  "module": "./index.qwik.mjs",
  "types": "./index.d.ts",
  "type": "module",
  "scripts": {
    "setup.chrome.113": "npx tsx ./scripts/prepare-chrome-113.ts"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.qwik.mjs",
      "require": "./index.qwik.cjs"
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "private": false,
  "peerDependencies": {
    "@builder.io/qwik": "^1.5.4"
  },
  "devDependencies": {
    "tsx": "4.7.2"
  }
}

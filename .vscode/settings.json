{
  "git.pullTags": false,
  "conventionalCommits.scopes": ["fluffy", "headless", "docs"],
  "tailwindCSS.experimental.classRegex": [
    ["cva\\(([^)]*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"]
  ],
  "editor.codeActionsOnSave": {
    "source.removeUnusedImports": true
  },
  "githubPullRequests.remotes": [
    "upstream"
  ],
  "cSpell.words": [
    "qwik"
  ]
}
